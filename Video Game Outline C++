            {
                cout << "NOTHING ADDED YET" << endl;
                goto audio_settings;
            }
            if (audio_settings_selection == 5)
            {
                goto all_settings;
            }
            if (audio_settings_selection < 1 || audio_settings_selection > 5)
            {
                cout << "Invalid Selection." << endl;
                goto audio_settings;
            }
        }
        else
        {
            goto menu;
        }
