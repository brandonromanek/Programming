        cout << "Slot 2: " << save_file2 << endl;
        cout << "Slot 3: " << save_file3 << endl;
        cout << "***************" << endl;
        cout << endl;
        cout << "Choose a save file (1, 2, 3): " << endl;
        int load_selection{0};
        cin >> load_selection;
        if (load_selection == 1)
        {
            if (save_file1 == "Empty")
            {
                cout << "Save file is empty." << endl;
                cout << endl;
                goto menu;
            }
            else
            {
                cout << save_file1 << " selected." << endl;
            }
            
