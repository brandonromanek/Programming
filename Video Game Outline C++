	        {
	            cout << "You take the revolver." << endl;
	            has_revolver = true;
	            goto guns;
	        }
	        if (revolver_choice != 'y' || revolver_choice != 'Y')
	        {
	            cout << "You decide not to take the revolver." << endl;
	            goto guns;
	        }
	    }
	    if (has_revolver == true || has_shotgun == true || has_ar == true)
	    {
	        cout << "You have already taken a weapon." << endl;
	        goto guns;
	    }
