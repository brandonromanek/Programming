    if (random_fight_start < 10)
    {
        cout << "You are under attack!" << endl;
        random_fight_outcome = rand() % 100 + 1;
        if (random_fight_outcome < 5)
        {
            cout << "You have died." << endl;
            goto end;
        }
        if (random_fight_outcome > 5)
        {
            cout << "You have won the fight!" << endl;
        }
    }
    if (random_fight_start > 10)
    {
        cout << "The area is clear." << endl;
    }
    cout << "(1) Northern Gate" << endl;
	cout << "(2) Escape Gate" << endl;
	
	cin >> terminal_choice;
	
	if (terminal_choice == 1)
	{
	    cout << "You go through the northern gate into the Junkyard." << endl;
	    goto junkyard;
	}
	if (terminal_choice == 2)
	{
	    cout << "You must enter a code into the terminal in order to escape." << endl;
	    if (has_code_note == false)
	    {
	        cout << "You have no idea what the code is..." << endl;
	        goto terminal;
	    }
	    if (has_code_note == true)
	    {
	        cout << "You look at the note: " << randomcode << endl;
	        cout << "Enter the code to escape: ";
	        int enterrandomcode;
	        cin >> enterrandomcode;
	        if (enterrandomcode == randomcode)
	        {
	            cout << "You have entered the correct code!" << endl;
	            cout << "You head into the escape area." << endl;
	            goto escape;
	        }
	        if (enterrandomcode != randomcode)
	        {
	            cout << "You have entered the incorrect code!" << endl;
	            goto terminal;
	        }
	    }
	}
	if (terminal_choice < 1 || terminal_choice > 2)
	{
	    cout << "Invalid Selection." << endl;
	    goto terminal;
	}
	
    //Escape
    escape:
    cout << "Congratulations! You have escaped the area!" << endl;
    goto end;
	
    
    
    
    
    end:
    return 0;
}
