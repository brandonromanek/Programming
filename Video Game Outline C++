        {
            cout << "You pry the door open with the crowbar." << endl;
            goto frontyard;
        }
        if (has_crowbar == false)
        {
            cout << "You pull on the door, but it will not open." << endl;
            goto start;
        }
        if (player_strength > 4 && has_crowbar == false)
        {
            cout << "You force open the door using brute force." << endl;
            goto frontyard;
        }
